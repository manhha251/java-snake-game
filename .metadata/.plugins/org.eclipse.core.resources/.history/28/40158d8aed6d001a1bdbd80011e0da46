package model;

import config.Config;

public class Point {

	private int x, y;
	
	public Point(int x, int y) {
		
		this.x = x;
		this.y = y;
	}
	
	public int getX() {
		
		return this.x;
	}
	
	public int getY() {
		
		return this.y;
	}
	
	public static Point getRandomPoint() {
		
		int random_x = (int)(Math.random());
	}
}
